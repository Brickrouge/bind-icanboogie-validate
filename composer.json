{
	"name": "brickrouge/bind-icanboogie-validate",
	"type": "library",
	"description": "icanboogie/validate bindings for Brickrouge",
	"keywords": [
		"validate",
		"element",
		"form"
	],
	"homepage": "https://brickrouge.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Brickrouge/bind-icanboogie-validate/issues",
		"source": "https://github.com/Brickrouge/bind-icanboogie-validate"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=5.5.0",
		"brickrouge/brickrouge": "^3.0",
		"icanboogie/validate": "^0.2"
	},
	"autoload": {
		"psr-4": {
			"Brickrouge\\Binding\\ICanBoogieValidate\\": "lib/"
		}
	},
	"extra": {
		"icanboogie": {
			"config-path": "config",
			"locale-path": "locale"
		}
	}
}
